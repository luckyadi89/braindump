{
  "name": "braindump",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:beta": "NODE_ENV=production NEXT_PUBLIC_APP_ENV=beta next dev",
    "dev:prod": "NODE_ENV=production NEXT_PUBLIC_APP_ENV=production next dev",
    "build": "next build",
    "build:beta": "NODE_ENV=production NEXT_PUBLIC_APP_ENV=beta next build",
    "build:prod": "NODE_ENV=production NEXT_PUBLIC_APP_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "echo \"Add your test command here\"",
    "test:ci": "npm run test",
    "test:full": "npm run test && npm run type-check && npm run lint",
    "test:e2e": "echo \"Add E2E test command here\"",
    "test:production": "npm run test:full && npm run test:e2e",
    "test:smoke-beta": "echo \"Add beta smoke tests here\"",
    "test:smoke-prod": "echo \"Add production smoke tests here\"",
    "analyze": "ANALYZE=true npm run build",
    "env:copy-dev": "cp .env.development .env.local",
    "env:copy-beta": "cp .env.beta .env.local",
    "env:copy-prod": "cp .env.production .env.local"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "lucide-react": "^0.453.0",
    "next": "15.5.0",
    "openai": "^4.68.4",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.5.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "15.5.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
